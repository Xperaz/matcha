{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "jest": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "prefer-const": 2,
    "no-unused-vars": 2,
    "no-var": 2,
    "no-undef": 2,
    "no-console": 2
  }
}
